        "/sign-up": {
            "post": {
                "tags": [
                    "AUTHENTICATION"
                ],
                "summary": "Signing Up a New Account For User",
                "description": "Signing Up with an email, password, and phone number with some policy in every user data",
                "requestBody": {
                    "required": true,
                    "content": {
                        "application/json": {
                            "schema": {
                                "type": "object",
                                "properties": {
                                    "name": {
                                        "type": "string",
                                        "required": true,
                                        "minLength": 5
                                    },
                                    "email": {
                                        "type": "string",
                                        "required": true
                                    },
                                    "phone_number": {
                                        "type": "integer",
                                        "required": true,
                                        "minLength": 12
                                    },
                                    "password": {
                                        "type": "string",
                                        "required": true,
                                        "minLength": 8
                                    }
